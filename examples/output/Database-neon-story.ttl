@prefix ENVO: <http://purl.obolibrary.org/obo/ENVO_> .
@prefix GOLD: <http://identifiers.org/gold/> .
@prefix MIXS: <https://w3id.org/mixs/> .
@prefix RetroRules: <https://unknown.to.linter.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix nmdc: <https://w3id.org/nmdc/> .

RetroRules:des1 a nmdc:Extraction ;
    nmdc:extraction_target "DNA" ;
    nmdc:has_inputs RetroRules:pool_out_1 ;
    nmdc:has_outputs RetroRules:dna_extract_1 ;
    nmdc:name "first dna extraction set" .

RetroRules:lcs1 a nmdc:LibraryPreparation ;
    nmdc:has_inputs RetroRules:dna_extract_1 ;
    nmdc:has_outputs RetroRules:library_1 ;
    nmdc:library_type "DNA" ;
    nmdc:name "DNA library preparation of NEON sample TREE_001-O-20170707-COMP-DNA1" .

RetroRules:ps1 a nmdc:Pooling ;
    dcterms:description "This is the first pooling process that has ever occurred on earth" ;
    nmdc:alternative_identifiers "bare:ps1_alt_id" ;
    nmdc:has_inputs nmdc:bsm-99-abcdef1,
        nmdc:bsm-99-abcdef2,
        nmdc:bsm-99-abcdef3 ;
    nmdc:has_outputs RetroRules:pool_out_1 ;
    nmdc:name "first pooling process" .

RetroRules:library_1 a nmdc:ProcessedSample ;
    nmdc:name "first library" .

nmdc:bsm-99-abcdef1 a nmdc:Biosample ;
    dcterms:isPartOf GOLD:Gs0110115 ;
    MIXS:0000012 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00002030" ;
            nmdc:term ENVO:00002030 ] ;
    MIXS:0000013 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00002169" ;
            nmdc:term ENVO:00002169 ] ;
    MIXS:0000014 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00005792" ;
            nmdc:term ENVO:00005792 ] .

nmdc:bsm-99-abcdef2 a nmdc:Biosample ;
    dcterms:isPartOf GOLD:Gs0110115 ;
    MIXS:0000012 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00002030" ;
            nmdc:term ENVO:00002030 ] ;
    MIXS:0000013 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00002169" ;
            nmdc:term ENVO:00002169 ] ;
    MIXS:0000014 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00005792" ;
            nmdc:term ENVO:00005792 ] .

nmdc:bsm-99-abcdef3 a nmdc:Biosample ;
    dcterms:isPartOf GOLD:Gs0110115 ;
    MIXS:0000012 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00002030" ;
            nmdc:term ENVO:00002030 ] ;
    MIXS:0000013 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00002169" ;
            nmdc:term ENVO:00002169 ] ;
    MIXS:0000014 [ a nmdc:ControlledIdentifiedTermValue ;
            nmdc:has_raw_value "ENVO:00005792" ;
            nmdc:term ENVO:00005792 ] .

ENVO:00002030 a nmdc:OntologyClass .

ENVO:00002169 a nmdc:OntologyClass .

ENVO:00005792 a nmdc:OntologyClass .

RetroRules:dna_extract_1 a nmdc:ProcessedSample ;
    nmdc:name "first DNA extract" .

RetroRules:pool_out_1 a nmdc:ProcessedSample ;
    nmdc:name "first processed sample set" .

[] a nmdc:Database ;
    nmdc:biosample_set nmdc:bsm-99-abcdef1,
        nmdc:bsm-99-abcdef2,
        nmdc:bsm-99-abcdef3 ;
    nmdc:extraction_set RetroRules:des1 ;
    nmdc:library_preparation_set RetroRules:lcs1 ;
    nmdc:pooling_set RetroRules:ps1 ;
    nmdc:processed_sample_set RetroRules:dna_extract_1,
        RetroRules:library_1,
        RetroRules:pool_out_1 .

